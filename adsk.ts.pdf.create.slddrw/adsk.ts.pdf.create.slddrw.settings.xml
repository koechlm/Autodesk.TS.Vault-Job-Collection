<?xml version="1.0" encoding="utf-8" ?>
<settings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <!-- Logfile directory; validate for write access -->
  <LogFileLocation>C:\Temp\</LogFileLocation>

  <!-- Export File Format(s)-->
  <!--  Supported Values/File Formats are: SLDDRW.PDF -->
  <ExportFormats>SLDDRW.PDF</ExportFormats>
  
  <!-- Include source file's extension in export file name, e.g. true: drawing.slddrw.pdf, false: drawing.pdf-->
  <IncludeSourceFileExtension>false</IncludeSourceFileExtension>
  
  <!-- Sheet Name for DXF Export; see option below, to include this sheet or exclude for PDF  -->
  <DxfSheetName>DXF</DxfSheetName>
  <!-- Include DXF Sheet for PDF Export; set to true or false -->
  <!--  If true, the sheet defined above will be included in the PDF export -->
  <PdfIncludeDxfSheet>false</PdfIncludeDxfSheet>

  <!-- Output Path -->
  <!--  The job can copy the resulting file(s) to the given output path after check-in-->
  <OutputPath>C:\Temp\</OutputPath>

</settings>